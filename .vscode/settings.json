{
  "editor.detectIndentation": false,
  "editor.insertSpaces": true,
  "editor.rulers": [80],
  "editor.tabSize": 4,
  "[json]": {
    "editor.tabSize": 2
  },
  "C_Cpp.default.includePath": [
    "${workspaceFolder}"
  ],
  "files.associations": {
    "*.h": "c"
  },
  "files.exclude": {
    "bazel-*": true
  },
  "C_Cpp.default.cppStandard": "c++17",
  "C_Cpp.default.cStandard": "c17"
}
